<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mac.swyhealthmonitoring">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PatientLogin" />
        <activity android:name=".DoctorLogin" />
        <activity android:name=".PatientForgetPassword" />
        <activity android:name=".DoctorForgetPassword" />
        <activity android:name=".PatientCreateAccount" />
        <activity android:name=".PatientCreateAccount2" />
        <activity android:name=".DoctorCreateAccount" />
        <activity android:name=".DoctorCreateAccount2" />
        <activity android:name=".PatientHome"></activity>
    </application>

</manifest>