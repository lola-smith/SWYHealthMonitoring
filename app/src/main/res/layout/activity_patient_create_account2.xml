<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mac.swyhealthmonitoring.PatientCreateAccount2">

    <ImageView
        android:id="@+id/SWYImageView"
        android:layout_width="192dp"
        android:layout_height="75dp"
        android:layout_marginBottom="132dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/MainPatientButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/swylogo" />

    <EditText
        android:id="@+id/PatientCreateAccount2UserName"
        android:layout_width="257dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="360dp"
        android:layout_marginEnd="85dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="35dp"
        android:ems="10"
        android:hint="@string/PatientCreateAccount2UserName"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SWYImageView" />

    <EditText
        android:id="@+id/PatientCreateAccount2Name"
        android:layout_width="262dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="262dp"
        android:layout_marginEnd="85dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="35dp"
        android:ems="10"
        android:hint="@string/PatientCreateAccount2Name"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PatientCreateAccount2UserName"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/PatientCreateAccount2Gender"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="175dp"
        android:layout_marginEnd="85dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="35dp"
        android:ems="10"
        android:hint="@string/PatientCreateAccount2Gender"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PatientCreateAccount2Name"
        app:layout_constraintVertical_bias="0.0" />

    <CheckBox
        android:id="@+id/PatientCreateAccount2Iagree"
        android:layout_width="262dp"
        android:layout_height="41dp"
        android:layout_marginBottom="98dp"
        android:layout_marginEnd="84dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="35dp"
        android:text="@string/PatientCreateAccount2Iagree"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PatientCreateAccount2Gender"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/PatientCreateAccount2CreateAccount"

        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="257dp"
        android:layout_height="57dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="85dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="35dp"
        android:text="@string/PatientCreateAccount2CreateAccount"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PatientCreateAccount2Iagree"
        app:layout_constraintVertical_bias="0.0" />


</android.support.constraint.ConstraintLayout>
