        android:background="?attr/colorButtonNormal"
        android:text="@string/PatientMyHealthTemperatureText"
        android:textColor="@android:color/background_dark"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/PatientMyHealthGlucoseText"
        app:layout_constraintEnd_toStartOf="@+id/PatientMyHealthTempReadText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/PatientMyHealthHeartRateText"
